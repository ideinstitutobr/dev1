{
    "name": "comercialdonorte/sgc-treinamentos",
    "description": "Sistema de Gestão de Capacitações - Gerenciamento de treinamentos corporativos",
    "type": "project",
    "license": "proprietary",
    "authors": [
        {
            "name": "Comercial do Norte",
            "email": "contato@comercialdonorte.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "phpmailer/phpmailer": "^6.8",
        "phpoffice/phpspreadsheet": "^1.29",
        "tecnickcom/tcpdf": "^6.6",
        "mpdf/mpdf": "^8.2",
        "guzzlehttp/guzzle": "^7.8"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
